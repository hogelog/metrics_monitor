{
  "compilerOptions": {
    "module": "es2015",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "target": "es5",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "lib": [
      "es2016", "es2017.object", "dom"
    ],
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
